# Copie este arquivo para .env e ajuste os valores.
# nunca commitar credenciais reais.

# Banco de dados
MYSQL_ROOT_PASSWORD=troque_root
DB_NAME=trentin_db
DB_USER=wpuser
DB_PASSWORD=troque_senha
DB_HOST=db:3306

# WordPress
WORDPRESS_DEBUG=1
WORDPRESS_TABLE_PREFIX=trentin_
# Opcional: em produção pode definir 'direct', 'ftpext' ou 'ssh2'
WORDPRESS_FS_METHOD=

# Salts (gere em https://api.wordpress.org/secret-key/1.1/salt/ )
AUTH_KEY=preencher
SECURE_AUTH_KEY=preencher
LOGGED_IN_KEY=preencher
NONCE_KEY=preencher
AUTH_SALT=preencher
SECURE_AUTH_SALT=preencher
LOGGED_IN_SALT=preencher
NONCE_SALT=preencher
